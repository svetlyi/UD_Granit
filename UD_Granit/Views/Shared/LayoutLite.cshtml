<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css/jquery.simple-dtpicker.css")
    @Styles.Render("~/Content/css/main.css")
	@Styles.Render("~/Content/css/lite.css")
    @Styles.Render("~/Content/themes/base/minified/jquery-ui.min.css")
    <script src="@Url.Content("~/Scripts/jquery-2.0.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.10.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/js/jquery.simple-dtpicker.js")" type="text/javascript"></script>
</head>
<body>
    <div id="MainWrapper">
        <header>
            @Html.Partial("_PartialHeader")
        </header>
        <aside>
            @Html.Partial("_PartialAside")
        </aside>
        <article>
            @Html.Partial("_PartialNotification")
            @RenderBody()
        </article>
        <div class="cb"></div>
        <footer>
            @Html.Partial("_PartialFooter")
        </footer>
    </div>
</body>
</html>
